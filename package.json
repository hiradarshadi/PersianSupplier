{
    "name": "PersianSupplier",
    "version": "2.0.0",
    "description": "Sketch Plugin for supplying dynamic persian datas (names, dates, ...)",
    "homepage": "https://github.com/hiradarshadi/PersianSupplier",
    "license": "MIT",
    "engines": {
        "sketch": ">=3.0"
    },
    "skpm": {
        "name": "PersianSupplier",
        "manifest": "src/manifest.json",
        "main": "persiansupplier.sketchplugin",
        "assets": [
            "assets/**/*"
        ]
    },
    "scripts": {
        "build": "skpm-build",
        "watch": "skpm-build --watch",
        "start": "skpm-build --watch --run",
        "postinstall": "npm run build && skpm-link"
    },
    "devDependencies": {
        "@skpm/builder": "^0.5.2"
    },
    "author": "Hirad Arshadi",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hiradarshadi/persiansupplier.git"
    },
    "dependencies": {
        "shortid": "^2.2.14",
        "skpm": "^1.1.8"
    }
}
