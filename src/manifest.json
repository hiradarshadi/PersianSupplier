{
    "name": "Persian Supplier",
    "description": "Sketch Plugin for supplying dynamic persian data (name, date, address, ...)",
    "author": "Hirad Arshadi",
    "authorEmail": "hiradwork@gmail.com",
    "compatibleVersion": 3,
    "homepage": "https://github.com/hiradarshadi/persiansupplier",
    "identifier": "com.box.persiansupplier",
    "bundleVersion": 1,
    "icon": "icon.png",
    "suppliesData": true,
    "commands": [
        {
            "script": "PersianSupplier.js",
            "handlers": {
                "actions": {
                    "Startup": "onStartup",
                    "Shutdown": "onShutdown",
                    "SupplyNames": "onSupplyNames",
                    "SupplyDates": "onSupplyDates",
                    "SupplyCities": "onSupplyCities",
                    "SupplyPhones": "onSupplyPhones",
                    "SupplyTime": "onSupplyTime",
                    "SupplyAddress": "onSupplyAddress",
                    "SupplyJobs": "onSupplyJobs",
                    "SupplyZipcodes": "onSupplyZipcodes",
                    "SupplyId": "onSupplyId",
                    "SupplyEmail": "onSupplyEmail"
                }
            }
        }
    ]
}
